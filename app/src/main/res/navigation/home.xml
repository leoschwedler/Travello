<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.mangami.features.main.home.presentation.ui.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_home2Fragment"
            app:destination="@id/home2Fragment" />
    </fragment>
    <fragment
        android:id="@+id/home2Fragment"
        android:name="com.example.mangami.Home2Fragment"
        android:label="fragment_home2"
        tools:layout="@layout/fragment_home2" />
</navigation>